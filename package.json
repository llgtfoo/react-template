{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.22.0",
    "@ant-design/pro-table": "^2.45.0",
    "antd": "^4.16.8",
    "querystring": "^0.2.1",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.5.13"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "^1.8.22",
    "@umijs/test": "^3.5.13",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react-dev-inspector": "^1.6.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  },
  "name": "react-template",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/llgtfoo/react-template.git",
  "author": "前端小丑 <lltfoo@163.com>",
  "license": "MIT"
}
